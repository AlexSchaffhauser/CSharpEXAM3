<Window x:Class="EXAM3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EXAM3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="First Name"/>
            <TextBox Name="FirstName" />
            <Label Content="Last Name"/>
            <TextBox Name="LastName" />
            <CheckBox Name="IsRegistered">Is Registered?</CheckBox>
            <Button Click="Button_Click">Save</Button>
            <Button Click="Button_Click_3">Show Entered Students</Button>
            <Button Margin="0, 20, 0, 0" Click="Button_Click_1">Run Question 2</Button>
            <Button Click="Button_Click_2">Run Question 3</Button>
        </StackPanel>
    </Grid>
</Window>
